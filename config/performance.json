{
    "mode": "speed",
    "extraction_depth": "basic",
    "confidence_threshold": 0.5,
    "parallel_processing": true,
    "max_workers": 4,
    "batch_size": 20,
    "save_temp_files": false,
    "validation": {
        "strict_mode": false,
        "auto_correct": true

{
    "formats": {
        "carte_identite": {
            "pattern": "^[0-9]{12}$",
            "description": "Numéro de carte d'identité française (12 chiffres)",
            "example": "123456789012"
        },
        "passeport": {
            "pattern": "^[0-9]{2}[A-Z]{2}[0-9]{5}$",
            "description": "Numéro de passeport français",
            "example": "12AB34567"
        },
        "siret": {
            "pattern": "^[0-9]{14}$",
            "description": "Numéro SIRET",
            "example": "12345678901234"
        },
        "siren": {
            "pattern": "^[0-9]{9}$",
            "description": "Numéro SIREN",
            "example": "123456789"
        },
        "nir": {
            "pattern": "^[12][0-9]{2}(0[1-9]|1[0-2])(2[AB]|[0-9]{2})[0-9]{8}$",
            "description": "Numéro de sécurité sociale (NIR)",
            "example": "187112B12345"
        }
    }
} 